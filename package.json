{
	"name": "talentify",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "tsc",
		"dev": "nodemon",
		"prestart": "npm run build",
		"start": "ts-node --project ./tsconfig.json -r tsconfig-paths/register dist/index.js",
		"postinstall": "npx prisma generate --schema=./src/prisma/schema.prisma",
		"db:generate": "npx prisma generate --schema=./src/prisma/schema.prisma",
		"db:push": "npx prisma db push --schema=./src/prisma/schema.prisma",
		"db:migrate": "npx prisma migrate dev --schema=./src/prisma/schema.prisma",
		"db:studio": "npx prisma studio --schema=./src/prisma/schema.prisma",
		"lint": "biome check .",
		"format": "biome check --write .",
		"prepare": "husky"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"type": "commonjs",
	"dependencies": {
		"@prisma/client": "^6.11.0",
		"bcrypt": "^6.0.0",
		"cloudinary": "^2.7.0",
		"cors": "^2.8.5",
		"dotenv": "^17.0.1",
		"express": "^5.1.0",
		"jsonwebtoken": "^9.0.2",
		"multer": "^2.0.1",
		"zod": "^3.25.67"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/multer": "^2.0.0",
		"@types/node": "^24.0.10",
		"husky": "^9.1.7",
		"nodemon": "^3.1.10",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.8.3"
	},
	"private": "true"
}
